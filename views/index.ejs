<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Trivia</title>
    <link rel="stylesheet" href="../public/css/styles.css">
</head>
<body>
    <header class="header">
        <h1 class="title">Trivia</h1>
    </header>
    <main>
        <section>
            <h2>My Decks</h2>
            <div class="view">
                <% data.forEach(function(deck){ %>
                    <a href="/trivia/1" class="card">
                        <div><%= deck.name %></div>
                    </a>
                <% }) %>
               
            </div>
        </section>
    </main>
    
</body>
</html>